@model Announcement
@{
    ViewData["Title"] = "AnnouncementDetails";
    Layout = "~/Views/Shared/_WriterLayout.cshtml";
}



<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Duyuru Detayaları</h4>
                <p class="card-description">
                    @* Add class <code>.table</code> *@
                </p>
                <form class="form-group">
                    <input type="hidden" value="@Model.AnnouncementID" />
                    <br />

                    <textarea rows="10" cols="3" class="form-control" disable asp-for="@Model.Content"> </textarea>
                </form>
            </div>
        </div>
    </div>
</div>
